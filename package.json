{
  "name": "restful-booker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env SEED=true node ./bin/www",
    "test": "mocha -R spec tests/spec.js",
    "build_docs": "apidoc -i ./routes/ -o public/apidoc/",
    "postman_workflow": "newman run ./postman/workflow/workflow.postman_collection.json -e ./postman/workflow/local.postman_environment.json -d ./postman/workflow/dataBookingsComplete.json --reporters cli,junit --reporter-junit-export ./reports/result.xml",
    "nodetests": "node tests/tests.js ../postman/environments/BookingLive.postman_environment.json",
    "nodetests-local": "node tests/tests.js ../postman/environments/BookingLocal.postman_environment.json"
  },
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gustavoviruega/restful-booker.git"
  },

  "author": "gustavoviruega",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gustavoviruega/restful-booker/issues"
  },

  "homepage": "https://github.com/gustavoviruega/restful-booker#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "form-urlencoded": "^4.1.3",
    "js2xmlparser": "^4.0.1",
    "morgan": "^1.10.0",
    "nedb": "^1.8.0",
    "newman": "^5.2.1",
    "pug": "^2.0.4",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.2",
    "supertest": "^4.0.2",
    "xml2js": "^0.4.23"
  }
}
